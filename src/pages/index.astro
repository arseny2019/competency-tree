---
import Layout from '../layouts/Layout.astro';
import CompetencyRoadmap from '../components/CompetencyRoadmap.jsx';
import dataEn from '../../data.json';
import dataRu from '../../data-ru.json';
---

<Layout title="Competency Tree">
  <main class="h-screen bg-gray-50">
    <div class="container mx-auto px-4 py-8">
      <CompetencyRoadmap dataEn={dataEn} dataRu={dataRu} client:load />
    </div>
  </main>
</Layout>
